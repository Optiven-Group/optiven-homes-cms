{
  "kind": "collectionType",
  "collectionName": "plans",
  "info": {
    "singularName": "plan",
    "pluralName": "plans",
    "displayName": "Plan",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "planMainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "planIntro": {
      "type": "text",
      "required": true,
      "maxLength": 300
    },
    "bedrooms": {
      "type": "integer",
      "required": true
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "planBanner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "planImages": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "planBackgroundBanner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "planContent": {
      "type": "blocks",
      "required": true
    },
    "propertyType": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-type.property-type"
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "finishTypes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::finish.finish"
    },
    "plinthArea": {
      "type": "biginteger",
      "required": true
    }
  }
}
